{
  "Logging": {
    "Region": "eu-west-2",
    "LogGroup": "Shares.Auth",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Password": {
    "IterationCount": 15000,
    "KeySize": 256,
    "SaltSize": 128,
    "Validation": {
      "RequiredLength": 6,
      "RequireUppercase": true,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": false,
      "RequireDigit": true,
      "RequiredUniqueChars": 0
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "database=shares;Data Source=10.119.1.235;User Id=root;Password=password;respect binary flags=False;Allow Zero Datetime=true;charset=utf8;"
  },
  "Hasher": {
    "KeyId":  "shares-jwt" 
  } 
}